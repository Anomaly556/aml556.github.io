{
    "css.lint.compatibleVendorPrefixes": "warning",
    "css.lint.duplicateProperties": "warning",
    "css.lint.hexColorLength": "warning",
    "css.lint.idSelector": "warning",
    "css.lint.ieHack": "error",
    "css.lint.important": "error",
    "css.lint.importStatement": "warning",
    "css.trace.server": "verbose",
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.useInlineCompletions": true,
    "html.format.indentInnerHtml": true,
    "html.trace.server": "verbose",
    "json.trace.server": "verbose",
    "markdown.occurrencesHighlight.enabled": true,
    "markdown.preview.breaks": false,
    "markdown.trace.extension": "verbose",
    "markdown.trace.server": "verbose",
    "markdown.validate.enabled": false,
    "material-icon-theme.folders.associations": {
        "_includes": "Include",
        "_layouts": "Layout",
        "_plugins": "Plugin",
        "workflows": "Tasks",
        "ext": "Plugin"
    },
    "material-icon-theme.files.associations": {
        "*.yml": "Yaml"
    },
    "editor.folding": false,
    "yaml.schemas": {
        "https://json.schemastore.org/jekyll": "/_config.yml"
    },
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorBlinking": "smooth",
    "diffEditor.codeLens": true,
    "diffEditor.diffAlgorithm": "experimental",
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.preview": true,
    "editor.suggest.showStatusBar": true,
    "files.autoGuessEncoding": true,
    "explorer.fileNesting.enabled": true,
    "explorer.compactFolders": false,
    "explorer.incrementalNaming": "smart",
    "problems.showCurrentInStatus": true,
    "javascript.referencesCodeLens.enabled": true,
    "yaml.suggest.parentSkeletonSelectedFirst": true,
    "yaml.trace.server": "verbose"
}